
<div class="modal fade" id="MultipleInscriptions" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel">@HIQResources.labelMultipleInscription</h4>
            </div>
            <div class="modal-body" id="body">
                @HIQResources.genericQuestionAreYouSure
            </div>
            <div class="modal-footer">
                <form id="ConfirmationForm" >
                    <button type="submit" class="btn btn-default btn-sm btn-remove" id="ConfirmationButton">@HIQResources.buttonConfirm</button> @*HAMMER TIME!*@
                    <button type="button" class="btn btn-default btn-sm btn-default" data-dismiss="modal">@HIQResources.buttonBack</button>
                </form>
            </div>
        </div>
    </div>
</div>

